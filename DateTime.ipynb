   "source": [
    "## How to write the date/time as a string"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "2020_10_24__18.05.41_hrs\n"
     ]
    }
   ],
   "source": [
    "from datetime import datetime\n",
    "\n",
    "\n",
    "\n",
    "now = datetime.now()\n",
    "datestr = now.strftime(\"%Y_%m_%d__%H.%M.%S_hrs\") # %H for 24-hour scheme\n",
    "\n",
    "print(datestr)"
   ]
  }
